{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "rootDir": "./src",
    "declaration": true,
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "composite": true,
    "declarationMap": true,
  },
  "include": [
    "src/**/*",
    "processLogTest.ts",
    "media/**/*"
  ],
  "exclude": [
    "node_modules",
    "processLogTest.ts"
  ]
}